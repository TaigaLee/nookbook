<%- include("../partials/header.ejs") %>

<h2>List of Users</h2>

<% if (message) { %>
	<div class="message-box">
		<%= message %>
	</div>
<% } %>

<div id='user-index'>
<div class="list-group">
    <% users.forEach((user) => { %>
        <a href="/user/<%= user._id %>" class="list-group-item">
            <img src="/user/<%= user._id %>/pic">
            <%= user.username %>
        </a>
    <% }) %>
</div>
</div>
<%- include("../partials/footer.ejs")%>
